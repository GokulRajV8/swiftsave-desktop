{
	"manifest_version": 3,
	"name": "SwiftSave for Desktop",
	"version": "0.1",
	"description": "Download photos and videos from Instagram posts and reels",

	"icons": {
		"128": "icon.png"
	},

	"content_scripts": [
		{
			"matches": [
				"https://www.instagram.com/p/*",
				"https://www.instagram.com/reel/*"
			],
			"js": [
				"scripts/main.js"
			]
		}
	]
}